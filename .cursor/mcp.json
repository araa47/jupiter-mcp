{
    "mcpServers": {
        "context7": {
            "command": "npx",
            "args": ["-y", "@upstash/context7-mcp@latest"]
        },
        "playwright": {
            "command": "npx",
            "args": [
                "@playwright/mcp@latest"
            ]
        },
        "jupiter-mcp": {
            "command": "npx",
            "args": [
              "envmcp",
              "--env-file",
              ".env",
              "uvx",
              "--from",
              "git+https://github.com/araa47/jupiter-mcp",
              "jupiter-mcp",
              "$SOLANA_RPC_URL",
              "$PRIVATE_KEY"
            ]
        }
    }
}
